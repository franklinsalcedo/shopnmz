n        <div className=\"item-count mx-auto mx-lg-0\">\n            <div className=\"quantity-item\">\n                <button className=\"btn-minus\" onClick={ handleClickMinus }>-</button>\n                <span className=\"quantity\">{ count }</span>\n                <button className=\"btn-plus\" onClick={ handleClickPlus }>+</button>\n            </div>\n            <button className=\"btn btn-dark\" onClick={ handledClickAdd }>Agregar al carrito</button>\n        </div>\n    );\n}\n\nexport default ItemCount;\n",["79","80"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ItemList/ItemList.jsx",["81"],"/Applications/MAMP/htdocs/shopnmz-react/src/index.js",[],["82","83"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/ItemDetail/ItemDetail.jsx",["84"],"import { useState,useEffect} from 'react';\nimport { useParams } from 'react-router-dom';\nimport ItemCount from '../common/ItemCount/ItemCount';\nimport { getFirestore } from '../../firebase';\nimport './ItemDetail.scss';\n\nfunction ItemDetail() {\n    const db = getFirestore();\n    const [message, setMessage] = useState('Cargando...'); \n    const { productId } = useParams();\n    const [product, setProduct] = useState();\n\n    useEffect(() => {\n        const itemCollection = db.collection('products');\n        const item = itemCollection.doc(productId);\n        item.get()\n        .then((doc) => {\n            if(!doc.exists) {\n                setMessage('El producto que busca no existe. Intente de nuevo con otro.');\n                return;\n            }\n            setProduct(doc.data());\n        })\n        .catch((error) => {\n            setMessage('No se puedo cargar el producto.');\n        })\n    },[]);\n\n    return(\n        <div className=\"container item-detail\">\n            <div className=\"row\">\n                {\n                    product ?\n                    <>\n                        <h1 className=\"col-12 d-lg-none mb-4 text-center\">{ product.title }</h1>\n                        <div className=\"col-12 col-lg-6 mb-4 mb-lg-0\">\n                            <img src={ '/images/'+product.image } className=\"img-fluid card-img-top\" alt={ product.title }/>\n                        </div>\n                        <div className=\"col-12 col-lg-6\">\n                            <h1 className=\"d-none d-lg-block mb-5\">{ product.title }</h1>\n                            <p className=\"price\">${ product.price }</p>\n                            <ItemCount stock={8} initial={1} itemId={ product.id } />\n                            <div className=\"py-5 description\">\n                                <h3>Descripción</h3>\n                                <p>{ product.description }</p>\n                                <p><strong>Material: </strong>{ product.material }</p>\n                                <p><strong>Color: </strong>{ product.color }</p>\n                            </div>\n                        </div>\n                    </>\n                    :\n                    <p className=\"col-12 text-center py-5\">{ message }</p>\n                }\n            </div>\n        </div>\n    );\n}\n\nexport default ItemDetail;","/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/itemsCart.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemsCart.jsx",["85","86","87","88","89","90","91"],"import { useState, useEffect, useContext } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { products } from '../../products';\nimport { Store } from '../../store';\nimport { FaTrashAlt } from 'react-icons/fa';\n\nfunction ItemsCart(props) {\n    const [data, setData] = useContext(Store);\n    const [listProducts, setProducts] = useState(products);\n    const [selects, setSelects] = useState(props.list);\n    const [total, setTotal] = useState(0);\n    const history = useHistory();\n\n    const handleClickMinus = (e) => {\n        console.log('quita');\n    }\n\n    const handleClickPlus = (e) => {\n        console.log('agrega');\n    }\n\n    const handleCheckout = (e) => {\n        history.push('/checkout');\n    }\n\n    useEffect(() => {\n        var priceTotal = 0;\n        selects.map((item) => (\n            priceTotal = priceTotal + (item.productData[0].price*item.qty)\n        ));\n        setTotal(priceTotal)\n    });\n    \n    return (\n        <div className=\"px-3\">\n            {\n                selects.map((item, index) => (\n                    <div key={index} className=\"row mb-4 h-100 row-items\">\n                        <div className=\"col-4 col-lg-1 p-0\">\n                            <img src={ '/images/'+item.productData[0].image } className=\"img-fluid\"/>\n                        </div>\n                        <div className=\"col-7 col-lg-5\">\n                            <p className=\"pt-4\"><strong>{ item.productData[0].title }</strong></p>\n                            <p>Precio: <strong>${ item.productData[0].price }</strong></p>\n                        </div>\n                        <div className=\"col-6 col-lg-3 pt-3 d-lg-flex align-items-center item-quantity\">\n                            <div className=\" quantity-item\">\n                                <button className=\"btn-minus\" onClick={ handleClickMinus }>-</button>\n                                <span className=\"quantity\">{ item.qty }</span>\n                                <button className=\"btn-plus\" onClick={ handleClickPlus }>+</button>\n                            </div>\n                        </div>\n                        <div className=\"col-6 col-lg-2 pt-3 d-lg-flex align-items-center justify-content-end text-right item-total\">\n                            <p>Precio: { item.price }</p>\n                        </div>\n                        <span className=\"ico-remove\"><FaTrashAlt /></span>\n                    </div>\n                ))\n            }\n            <div className=\"row my-5\">\n                <div className=\"col-12 text-right\">\n                    Total a pagar: ${ total }\n                </div>\n            </div>\n            <div className=\"row\">\n                <div className=\"col-12 text-lg-right\">\n                    <button onClick={ handleCheckout } className=\"btn btn-dark\">Ir al Checkout</button>\n                </div>\n            </div>\n        </div>\n    );\n}\n\nexport default ItemsCart;","/Applications/MAMP/htdocs/shopnmz-react/src/components/Checkout/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/Footer/Footer.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/reportWebVitals.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Category/Category.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ProductThumb/ProductThumb.jsx",["92"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/NavBar/index.jsx",["93"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/FeaturedProducts.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/Slider.jsx",[],{"ruleId":"94","severity":1,"message":"95","line":9,"column":18,"nodeType":"96","messageId":"97","endLine":9,"endColumn":25},{"ruleId":"98","severity":1,"message":"99","line":27,"column":7,"nodeType":"100","endLine":27,"endColumn":9,"suggestions":"101"},{"ruleId":"94","severity":1,"message":"102","line":3,"column":10,"nodeType":"96","messageId":"97","endLine":3,"endColumn":18},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","replacedBy":"106"},{"ruleId":"98","severity":1,"message":"107","line":36,"column":7,"nodeType":"100","endLine":36,"endColumn":9,"suggestions":"108"},{"ruleId":"103","replacedBy":"109"},{"ruleId":"105","replacedBy":"110"},{"ruleId":"98","severity":1,"message":"111","line":27,"column":7,"nodeType":"100","endLine":27,"endColumn":9,"suggestions":"112"},{"ruleId":"94","severity":1,"message":"113","li