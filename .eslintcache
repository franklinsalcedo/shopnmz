lassName=\"container\" id=\"cart\">\n            <div className=\"row mb-5\">\n                <div className=\"col-12\">\n                    <h1 className=\"text-center\">Cart</h1>\n                </div>\n            </div>\n            {\n                selects.length ?\n                    <ItemsCart list={ selects } />\n                :\n                    <div className=\"row\">\n                        <div className=\"col-12 text-center\">\n                            <p>Carrito vacío...</p>\n                        </div>\n                    </div>\n            }\n        </section>\n    );\n}\n\nexport default Cart;\n","/Applications/MAMP/htdocs/shopnmz-react/src/App.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ItemCount/ItemCount.jsx",["82","83","84"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ItemList/ItemList.jsx",["85","86","87"],"/Applications/MAMP/htdocs/shopnmz-react/src/index.js",[],["88","89"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/itemsCart.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemsCart.jsx",["90","91","92","93","94","95","96"],"import { useState, useEffect, useContext } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { products } from '../../products';\nimport { Store } from '../../store';\nimport { FaTrashAlt } from 'react-icons/fa';\n\nfunction ItemsCart(props) {\n    const [data, setData] = useContext(Store);\n    const [listProducts, setProducts] = useState(products);\n    const [selects, setSelects] = useState(props.list);\n    const [total, setTotal] = useState(0);\n    const history = useHistory();\n\n    const handleClickMinus = (e) => {\n        console.log('quita');\n    }\n\n    const handleClickPlus = (e) => {\n        console.log('agrega');\n    }\n\n    const handleCheckout = (e) => {\n        history.push('/checkout');\n    }\n\n    useEffect(() => {\n        var priceTotal = 0;\n        selects.map((item) => (\n            priceTotal = priceTotal + (item.productData[0].price*item.qty)\n        ));\n        setTotal(priceTotal)\n    });\n    \n    return (\n        <div className=\"px-3\">\n            {\n                selects.map((item, index) => (\n                    <div key={index} className=\"row mb-4 h-100 row-items\">\n                        <div className=\"col-4 col-lg-1 p-0\">\n                            <img src={ '/images/'+item.productData[0].image } className=\"img-fluid\"/>\n                        </div>\n                        <div className=\"col-7 col-lg-5\">\n                            <p className=\"pt-4\"><strong>{ item.productData[0].title }</strong></p>\n                            <p>Precio: <strong>${ item.productData[0].price }</strong></p>\n                        </div>\n                        <div className=\"col-6 col-lg-3 pt-3 d-lg-flex align-items-center item-quantity\">\n                            <div className=\" quantity-item\">\n                                <button className=\"btn-minus\" onClick={ handleClickMinus }>-</button>\n                                <span className=\"quantity\">{ item.qty }</span>\n                                <button className=\"btn-plus\" onClick={ handleClickPlus }>+</button>\n                            </div>\n                        </div>\n                        <div className=\"col-6 col-lg-2 pt-3 d-lg-flex align-items-center justify-content-end text-right item-total\">\n                            <p>Precio: { item.price }</p>\n                        </div>\n                        <span className=\"ico-remove\"><FaTrashAlt /></span>\n                    </div>\n                ))\n            }\n            <div className=\"row my-5\">\n                <div className=\"col-12 text-right\">\n                    Total a pagar: ${ total }\n                </div>\n            </div>\n            <div className=\"row\">\n                <div className=\"col-12 text-lg-right\">\n                    <button onClick={ handleCheckout } className=\"btn btn-dark\">Ir al Checkout</button>\n                </div>\n            </div>\n        </div>\n    );\n}\n\nexport default ItemsCart;",["97","98"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Checkout/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/Footer/Footer.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/reportWebVitals.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ProductThumb/ProductThumb.jsx",["99"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/NavBar/index.jsx",["100"],"import { useState, useEffect } from 'react';\nimport { NavLink } from 'react-router-dom';\nimport { getFirestore } from '../../../firebase'\nimport CartWidget from '../../Cart/CartWidget';\nimport './NavBar.scss';\nimport { RiMenu2Fill } from 'react-icons/ri';\nimport Logo from '../../../assets/logo-nmz.png';\n\nfunction NavBar() {\n    const db = getFirestore();\n    const [categories, setCategories] = useState([]);\n\n    const getListCategories = () => {\n        const listCategories = db.collection('categories');\n        let arr = [];\n        listCategories.orderBy('order').get()\n        .then((docs) => {\n            if(docs.size === 0) {\n                console.log('No existen las categorías');\n            }\n            docs.forEach(doc => {\n                arr.push(doc.data());\n            });\n            setCategories(arr);\n        })\n        .catch((error) => {\n            console.log(error);\n        })\n    }\n\n    useEffect(() => {\n        getListCategories();\n    },[])\n\n    return (\n        <header>\n            <div className=\"container\">\n                <div className=\"row py-3\">\n                    <div className=\"mr-auto ml-3 d-lg-none menu-icon-mobile\">\n                        <RiMenu2Fill />\n                    </div>\n                    <div className=\"col-4 pt-2 text-center text-lg-left logo\">\n                        <NavLink to=\"/\">\n                            <img src={Logo} alt=\"Shop NMZ\" className=\"img-fluid\" />\n                        </NavLink>\n                    </div>\n                    <div className=\"ml-auto pt-2 align-self-center\">\n                        <CartWidget/>\n                    </div>\n                </div>\n            </div>\n            <nav id=\"mainMenu\" className=\"d-none d-lg-block\">\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <div className=\"col-12\">\n                            <ul className=\"nav\">\n                                <li className=\"nav-item\"><NavLink to=\"/\" className=\"nav-link\" exact>Inicio</NavLink></li>\n                                {\n                                    categories.map((category,i) => (\n                                        <li className=\"nav-item\" key={i}><NavLink to={{ pathname: `/categoria/${ category.handle }` }} className=\"nav-link\">{category.title}</NavLink></li>\n                                    ))\n                                }\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </nav>\n        </header>\n    );\n}\n\nexport default NavBar;\n",["101","102"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/FeaturedProducts.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/Slider.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Category/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/ItemDetail/index.jsx",["103"],"/Applications/MAMP/htdocs/shopnmz-react/src/store/index.js",[],{"ruleId":"104","severity":1,"message":"105","line":9,"column":18,"nodeType":"106","messageId":"107","endLine":9,"endColumn":25},{"ruleId":"108","severity":1,"message":"1