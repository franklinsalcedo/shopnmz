","errorCount":0,"warningCount":4,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/Cart.jsx",["98"],"/Applications/MAMP/htdocs/shopnmz-react/src/App.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ItemCount/ItemCount.jsx",["99","100","101"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ItemList/ItemList.jsx",["102"],"import { useState,useEffect } from 'react';\nimport { useParams } from 'react-router-dom';\nimport ProductThumb from '../ProductThumb/ProductThumb';\nimport { getFirestore } from '../../../firebase';\nimport './ItemList.scss';\n\nfunction ItemList(props) {\n    let limit = props.quantity;\n    const db = getFirestore();\n    const { categoryHandle } = useParams();\n    const [items, setItems] = useState([]);\n    const [message, setMessage] = useState('Cargando...');\n\n    const getProducts = async (cat) => {\n        setMessage('Cargando...');\n        let itemCollection = (cat) ? db.collection(\"products\").where(\"category\", \"==\", cat) : db.collection('products');\n        let arr = [];\n        itemCollection.get()\n        .then((docs) => {\n            if(docs.size === 0) {\n                setMessage('Disculpa, no tenemos productos que mostrar en este momento.');\n            }\n            docs.forEach(doc => {\n                arr.push({\n                    id: doc.id,\n                    data: doc.data()\n                });\n            })\n            setItems(arr);\n            //setItems(querySnapshot.docs.map(doc => doc.data()));\n        }).catch((error) => {\n            setMessage('Disculpa, hubo un error cargando los productos');\n            //console.log(\"Error searching items\", error);\n        })\n        console.log('list items');\n    }\n    \n    useEffect(() => {\n        getProducts(categoryHandle);\n    },[categoryHandle]);\n\n    return (\n        <div className=\"row py-3 py-md-5\">\n            {\n                items.length ?\n                    <>\n                        {\n                            items.slice(0,limit).map((item) => (\n                                <ProductThumb key={item.id} id={item.id} title={item.data.title} price={item.data.price} handle={item.data.handle} image={item.data.image} />\n                            ))\n                        }\n                    </>\n                :\n                    <p className=\"col-12 text-center py-5\">\n                        { message }\n                    </p>\n            }\n        </div>\n    );\n}\n\nexport default ItemList;","/Applications/MAMP/htdocs/shopnmz-react/src/index.js",[],["103","104"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Checkout/index.jsx",["105","106","107","108","109"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/Footer/Footer.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/reportWebVitals.js",[],["110","111"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ProductThumb/ProductThumb.jsx",["112"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/NavBar/index.jsx",["113"],"import { useState, useEffect } from 'react';\nimport { NavLink } from 'react-router-dom';\nimport { getFirestore } from '../../../firebase'\nimport CartWidget from '../../Cart/CartWidget';\nimport './NavBar.scss';\nimport { RiMenu2Fill } from 'react-icons/ri';\nimport Logo from '../../../assets/logo-nmz.png';\n\nfunction NavBar() {\n    const db = getFirestore();\n    const [categories, setCategories] = useState([]);\n\n    const getListCategories = () => {\n        const listCategories = db.collection('categories');\n        let arr = [];\n        listCategories.orderBy('order').get()\n        .then((docs) => {\n            if(docs.size === 0) {\n                console.log('No existen las categorías');\n            }\n            docs.forEach(doc => {\n                arr.push(doc.data());\n            });\n            setCategories(arr);\n        })\n        .catch((error) => {\n            console.log(error);\n        })\n    }\n\n    useEffect(() => {\n        getListCategories();\n    },[])\n\n    return (\n        <header>\n            <div className=\"container\">\n                <div className=\"row py-3\">\n                    <div className=\"mr-auto ml-3 d-lg-none menu-icon-mobile\">\n                        <RiMenu2Fill />\n                    </div>\n                    <div className=\"col-4 pt-2 text-center text-lg-left logo\">\n                        <NavLink to=\"/\">\n                            <img src={Logo} alt=\"Shop NMZ\" className=\"img-fluid\" />\n                        </NavLink>\n                    </div>\n                    <div className=\"ml-auto pt-2 align-self-center\">\n                        <CartWidget/>\n                    </div>\n                </div>\n            </div>\n            <nav id=\"mainMenu\" className=\"d-none d-lg-block\">\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <div className=\"col-12\">\n                            <ul className=\"nav\">\n                                <li className=\"nav-item\"><NavLink to=\"/\" className=\"nav-link\" exact>Inicio</NavLink></li>\n                                {\n                                    categories.map((category,i) => (\n                                        <li className=\"nav-item\" key={i}><NavLink to={{ pathname: `/categoria/${ category.handle }` }} className=\"nav-link\">{category.title}</NavLink></li>\n                                    ))\n                                }\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </nav>\n        </header>\n    );\n}\n\nexport default NavBar;\n",["114","115"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/FeaturedProducts.jsx",[],["116","117"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/Slider.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Category/index.jsx",[],["118","119"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/ItemDetail/index.jsx",["120"],"/Applications/MAMP/htdocs/shopnmz-react/src/store/index.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/products.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/CartWidget.jsx",["121"],"import { useState, useEffect, useContext } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { Store } from '../../store';\nimport ItemCartWidget from './ItemCartWidget';\nimport { BiShoppingBag } from 'react-icons/bi';\nimport { IoCloseCircleOutline } from 'react-icons/io5';\nimport './CartWidget.scss';\n\nfunction CartWidget() {\n    const history = useHistory();\n    const [data, setData] = useContext(Store);\n    const [open, setOpen] = useState(false);\n    const [products, setProducts] = useState(data.items);\n    const [total, setTotal] = useState(0);\n\n    const handleOpenClose = (e) => {\n        showAddCart();\n    }\n\n    const handleKeyDown = (e) => {\n        e.keyCode === 27 && showAddCart();\n    }\n\n    const showAddCart = (o=false) => {\n        if(window.location.href.indexOf(\"/cart\") < 0 && window.location.href.indexOf(\"/checkout\") < 0) {\n            if(o)\n                setOpen(true);\n            else\n                setOpen(!open);\n        }\n    }\n\n    const handleGoCart = (e) => {\n        showAddCart();\n        history.push('/cart');\n    }\n\n    const getItems = (obj) => {\n        let rowItems = Object.keys(products).map(key => (<ItemCartWidget key={key} details={products[key]} />));\n        return (rowItems);\n    }\n\n    const getTotal = () => {\n        var totalTemp = 0;\n        Object.keys(data.items).map(key => (\n            totalTemp = totalTemp + data.items[key].subtotal\n        ))\n        if(totalTemp !== data.totalAmount) {\n            setData({\n                ...data,\n                totalAmount: totalTemp\n            });\n        }\n        setTotal(totalTemp);\n    }\n\n    useEffect(() => {\n        window.addEventListener('keydown', handleKeyDown);\n        setProducts(data.items);\n        getTotal();\n        if(data.items.length)\n            showAddCart(true);\n    }, [data,total]);\n\n    return (\n        <div className=\"mr-3 mr-lg-4 p-lg-2 \">\n            <div className=\"icon-cart\" onClick={ handleOpenClose }>\n                <span className=\"total-items-counter\">\n                    { data.qtyItems }\n                </span>\n                <BiShoppingBag />\n            </div>\n            <div id=\"cover\" className={ open ? \"open\" : null } onClick={ handleOpenClose }></div>\n            <div id=\"slidecart\" className={ open ? \"open\" : null }>\n                <div className=\"container h-100\">\n                    <div className=\"row\">\n                        <div className=\"col-12 py-3 header\">\n                            <h3 className=\"m-0\">Cart</h3>\n                            <span className=\"close\" onClick={ handleOpenClose }>\n                                <IoCloseCircleOutline />\n                            </span>\n                        </div>\n                    </div>\n                    <div className=\"container p-0 pt-4 h-100\">\n                        {\n                            Object.keys(products).length > 0 ?\n                                <div className=\"row h-100\">\n                                    <div className=\"col-12 h-100 list-items\">\n                                        { getItems(products) }\n                                    </div>\n                                    \n                                    <div className=\"row footer-widget\">\n                                        <div className=\"mb-4 col-12 text-center\">\n                                            Total a pagar: <strong>${ total }</strong>\n                                        </div>\n                                        <div className=\"col-12 text-center\">\n                                            <button onClick={ handleGoCart } className=\"btn btn-dark\">Ir al Cart</button>\n                                        </div>\n                                    </div>\n                                </div>\n                            :\n                            <div className=\"row\">\n                                <p className=\"col-12 py-5 text-center\">Carrito vacío...</p>\n                            </div>\n                        }\n                    </div>\n                </div>\n            </div>\n        </div>\n    );\n};\n\nexport default CartWidget\n","/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemCart.jsx",["122","123","124","125"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemCartWidget.jsx",["126","127","128","129"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/Error404/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/ConsultOrder/index.jsx",["130","131","132","133"],{"ruleId":"134","severity":1,"message":"135","line":41,"column":7,"nodeType":"136","endLine":41,"endColumn":19,"suggestions":"137"},{"ruleId":"138","severity":1,"message":"139","line":12,"column":19,"nodeType":"140","messageId":"141","endLine":12,"endColumn":27},{"ruleId":"138","severity":1,"message":"142","line":16,"column":11,"nodeType":"140","messageId":"141","endLine":16,"endColumn":18},{"ruleId":"134","severity":1,"message":"143","line":75,"column":8,"nodeType":"136","endLine":75,"endColumn":10,"suggestions":"144"},{"ruleId":"134","severity":1,"message":"145","line":40,"column":7,"nodeType":"136","endLine":40,"endColumn":23,"suggestions":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"138","severity":1,"messa