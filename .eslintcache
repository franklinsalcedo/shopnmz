unt.jsx",["92","93","94"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ItemList/ItemList.jsx",["95"],"import { useState,useEffect } from 'react';\nimport { useParams } from 'react-router-dom';\nimport ProductThumb from '../ProductThumb/ProductThumb';\nimport { getFirestore } from '../../../firebase';\nimport './ItemList.scss';\n\nfunction ItemList(props) {\n    let limit = props.quantity;\n    const db = getFirestore();\n    const { categoryHandle } = useParams();\n    const [items, setItems] = useState([]);\n    const [message, setMessage] = useState('Cargando...');\n\n    const getProducts = async (cat) => {\n        setMessage('Cargando...');\n        let itemCollection = (cat) ? db.collection(\"products\").where(\"category\", \"==\", cat) : db.collection('products');\n        let arr = [];\n        itemCollection.get()\n        .then((docs) => {\n            if(docs.size === 0) {\n                setMessage('Disculpa, no tenemos productos que mostrar en este momento.');\n            }\n            docs.forEach(doc => {\n                arr.push({\n                    id: doc.id,\n                    data: doc.data()\n                });\n            })\n            setItems(arr);\n            //setItems(querySnapshot.docs.map(doc => doc.data()));\n        }).catch((error) => {\n            setMessage('Disculpa, hubo un error cargando los productos');\n            //console.log(\"Error searching items\", error);\n        })\n        console.log('list items');\n    }\n    \n    useEffect(() => {\n        getProducts(categoryHandle);\n    },[categoryHandle]);\n\n    return (\n        <div className=\"row py-3 py-md-5\">\n            {\n                items.length ?\n                    <>\n                        {\n                            items.slice(0,limit).map((item) => (\n                                <ProductThumb key={item.id} id={item.id} title={item.data.title} price={item.data.price} handle={item.data.handle} image={item.data.image} />\n                            ))\n                        }\n                    </>\n                :\n                    <p className=\"col-12 text-center py-5\">\n                        { message }\n                    </p>\n            }\n        </div>\n    );\n}\n\nexport default ItemList;","/Applications/MAMP/htdocs/shopnmz-react/src/index.js",[],["96","97"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Checkout/index.jsx",["98","99","100","101","102","103"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/Footer/Footer.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/reportWebVitals.js",[],["104","105"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/ProductThumb/ProductThumb.jsx",["106"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/NavBar/index.jsx",["107"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/FeaturedProducts.jsx",[],["108","109"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/Slider.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Category/index.jsx",[],["110","111"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/ItemDetail/index.jsx",["112"],"/Applications/MAMP/htdocs/shopnmz-react/src/store/index.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/products.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/CartWidget.jsx",["113"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemCart.jsx",["114","115","116","117"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemCartWidget.jsx",["118","119","120","121"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/common/Error404/index.jsx",[],{"ruleId":"122","severity":1,"message":"123","line":41,"column":7,"nodeType":"124","endLine":41,"endColumn":19,"suggestions":"125"},{"ruleId":"126","severity":1,"message":"127","line":12,"column":19,"nodeType":"128","mes