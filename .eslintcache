CartWidget/>\n                    </div>\n                </div>\n            </div>\n            <nav id=\"mainMenu\" className=\"d-none d-lg-block\">\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <div className=\"col-12\">\n                            <ul className=\"nav\">\n                                <li className=\"nav-item\"><NavLink to=\"/\" className=\"nav-link\" exact>Inicio</NavLink></li>\n                                {\n                                    categories.map((category,i) => (\n                                        <li className=\"nav-item\" key={i}><NavLink to={{ pathname: `/categoria/${ category.handle }` }} className=\"nav-link\">{category.title}</NavLink></li>\n                                    ))\n                                }\n                            </ul>\n                        </div>\n                    </div>\n                </div>\n            </nav>\n        </header>\n    );\n}\n\nexport default NavBar;\n",["96","97"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/FeaturedProducts.jsx",[],["98","99"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Home/Slider.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Category/index.jsx",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/ItemDetail/index.jsx",["100"],"import { useState,useEffect} from 'react';\nimport { useParams } from 'react-router-dom';\nimport ItemCount from '../common/ItemCount/ItemCount';\nimport { getFirestore } from '../../firebase';\nimport './ItemDetail.scss';\n\n\nfunction ItemDetail() {\n    const db = getFirestore();\n    const [product, setProduct] = useState();\n    const [message, setMessage] = useState('Cargando...');\n    const { productHandle } = useParams();\n\n    const getProduct = () => {\n        const item = db.collection('products').where('handle', '==', productHandle);\n        let arr = []\n\n        item.get()\n        .then((docs) => {\n            if(docs.size === 0) {\n                setMessage('Disculpa, el producto que buscas no estÃ¡ disponible.');\n            }\n            docs.forEach(function(doc) {\n                arr.push({\n                    id: doc.id,\n                    data: doc.data()\n                });\n            });\n            setProduct(arr[0]);\n        })\n        .catch(function(error) {\n            setMessage('Disculpa, hubo un error cargando el producto');\n        });\n    }\n    \n    useEffect(() => {\n        getProduct();\n    },[]);\n\n    return(\n        <div className=\"py-5 container item-detail\">\n            <div className=\"row\">\n                {\n                    product ?\n                    <>\n                        <h1 className=\"col-12 d-lg-none mb-4 text-center\">{ product.data.title }</h1>\n                        <div className=\"col-12 col-lg-6 mb-4 mb-lg-0\">\n                            <img src={ '/images/'+product.data.image } className=\"img-fluid card-img-top\" alt={ product.data.title }/>\n                        </div>\n                        <div className=\"col-12 col-lg-6\">\n                            <h1 className=\"d-none d-lg-block\">{ product.data.title }</h1>\n                            <p><strong>CÃ³digo de producto:</strong> { product.id }</p>\n                            <p className=\"price\">${ product.data.price }</p>\n                            <div className=\"description\">\n                                <p>{ product.data.description }</p>\n                                {\n                                    product.data.material &&\n                                        <p><strong>Material: </strong>{ product.data.material }</p>\n                                }\n                            </div>\n                            <ItemCount initial={ 1 } itemId={ product.id } itemSizes={ product.data.size } itemPrice={ product.data.price } />\n                        </div>\n                    </>\n                    :\n                    <p className=\"col-12 text-center py-5\">{ message }</p>\n                }\n            </div>\n        </div>\n    );\n}\n\nexport default ItemDetail;\n","/Applications/MAMP/htdocs/shopnmz-react/src/store/index.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/products.js",[],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/CartWidget.jsx",["101","102"],"/Applications/MAMP/htdocs/shopnmz-react/src/components/Cart/ItemCart.jsx",["103","104"],{"ruleId":"105","severity":1,"message":"106","line":41,"column":7,"nodeType":"107","endLine":41,"endColumn":19,"suggestions":"108"},{"ruleId":"109","severity":1,"message":"110","line":1,"column":20,"nodeType":"111","messageId":"112","endLine":1,"endColumn":27},{"ruleId":"109","severity":1,"message":"113","line":12,"column":19,"nodeType":"111","messageId":"112","endLine":12,"endColumn":27},{"ruleId":"105","severity":1,"message":"114","line":76,"column":8,"nodeType":"107","endLine":76,"endColumn":10,"suggestions":"115"},{"ruleId":"105","severity":1,"message":"116","line":40,"column":7,"nodeType":"107","endLine":40,"endColumn":23,"suggestions":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"118","replacedBy":"122"},{"ruleId":"120","replacedBy":"123"},{"ruleId":"109","severity":1,"message":"124","line":3,"column":8,"nodeType":"111","messageId":"112","endLine":3,"endColumn":17},{"ruleId":"105","severity":1,"message":"125","line":33,"column":7,"nodeType":"107","endLine":33,"endColumn":9,"suggestions":"126"},{"ruleId":"118","replacedBy":"127"},{"ruleId":"120","replacedBy":"128"},{"ruleId":"118","replacedBy":"129"},{"ruleId":"120","replacedBy":"130"},{"ruleId":"105","severity":1,"message":"131","line":38,"column":7,"nodeType":"107","endLine":38,"endColumn":9,"suggestions":"132"},{"ruleId":"109","severity":1,"message":"133","line":9,"